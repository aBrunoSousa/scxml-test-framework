<?xml version="1.0" encoding="UTF-8"?>
<!-- orthogonal preemption - outer and state interrupts inner or state -->
<scxml
	xmlns="http://www.w3.org/2005/07/scxml"
	version="1.0"
	profile="ecmascript"
	id="root"
	initial="b">

	<parallel id="b">
		<state id="c"/>

		<state id="d">
			<transition event="t" target="a2"/>
		</state>

		<transition event="t" target="a1"/>
	</parallel>

	<state id="a1"/>

	<state id="a2"/>
</scxml>



