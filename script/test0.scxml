<?xml version="1.0" encoding="UTF-8"?>
<scxml 
	xmlns="http://www.w3.org/2005/07/scxml"
	version="1.0"
	profile="ecmascript"
	id="root">

	<initial id="intitial1">
		<transition target="a">
			<script>
				setData('a',100)
			</script>
		</transition>
	</initial>

	<state id="a">
		<transition target="b" event="t" cond="getData('a') === 100"/>
		<transition target="f" event="t" />
	</state>

	<state id="b"/>

	<state id="f"/>


</scxml>




