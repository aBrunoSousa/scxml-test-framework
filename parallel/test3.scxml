<!-- nested orthogonality 
initial configuration: [s3.1,s4,s7,s8]
after event t: [s3.2,s4,s9,s10]
-->
<scxml
	xmlns="http://www.w3.org/2005/07/scxml"
	version="1.0"
	profile="ecmascript"
	id="root"
	initial="p1">

	<parallel id="p1">
		<state id="s1" initial="p2">
			<parallel id="p2">
				<state id="s3" initial="s3.1">
					<state id="s3.1">
						<transition target="s3.2" event="t"/>
					</state>

					<state id="s3.2"/>
				</state>

				<state id="s4">
				</state>

			</parallel>

			<parallel id="p3">
				<state id="s5">
				</state>

				<state id="s6">
				</state>
			</parallel>

		</state>

		<state id="s2" initial="p4">
			<parallel id="p4">
				<state id="s7">
				</state>

				<state id="s8">
				</state>

				<transition target="p5" event="t"/>
			</parallel>

			<parallel id="p5">
				<state id="s9">
				</state>

				<state id="s10">
				</state>
			</parallel>
		</state>

	</parallel>

</scxml>

